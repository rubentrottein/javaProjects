package model;

public class Joueur {
	int y; 
	int z; 
	char sprite;
	public Joueur(int y, int z, char sprite) {
		super();
		this.y = y;
		this.z = z;
		this.sprite = sprite;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int getZ() {
		return z;
	}
	public void setZ(int z) {
		this.z = z;
	}
	public char getSprite() {
		return sprite;
	}
	public void setSprite(char sprite) {
		this.sprite = sprite;
	}
	
}