package game;

public class Game {

	static String[] board = new String[10];
	
	public static void launch() {
		System.out.println(displayBoard(board));
		
	}
	public static String displayBoard(String[] tab) {
		String board = "";
		for(int i = 0; i<tab.length; i++) {
			tab[i] = "-";
			board += "-";
		}
		return board;
	}
}
