package test;

public class StringExos {
	public static void main(String[] args) {
		String s = "Hello";
		
		write(s);
		
		
	}
	public static String write(String s) {
		System.out.println(s);
		return s;
	}
	public static String inverseString(String stringToInverse) {
		char string = String[stringToInverse.toCharArray().length];
		for(char c : string) {			
			return stringToInverse;
		}
	}
}
