package model;

public class Joueur {
	int y; 
	int x; 
	String sprite;
	public Joueur(int y, int x, String sprite) {
		super();
		this.y = y;
		this.x = x;
		this.sprite = sprite;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public String getSprite() {
		return sprite;
	}
	public void setSprite(char sprite) {
		this.sprite = sprite;
	}
	
}